{% extends "layouts/layout.jinja" %}
{% from "components/step-by-step-nav/macro.jinja" import step_by_step_nav %}
{% from "components/breadcrumbs.jinja" import breadcrumbs %}
{% from "components/panel/macro.njk" import govukPanel %}

{% set page_title = "Import commodities" %}


{% block breadcrumb %}
  {{ breadcrumbs(request, [
    {"text": "Import commodities", "href": url("commodity-ui-import")},
    {"text": page_title}
  ])}}
{% endblock %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      {{ govukPanel({
        "titleText": "TARIC ID " ~ saved_file_name ~ " has been uploaded",
        "text": "Workbasket " ~ saved_file_workbasket_id ~ " created",
        "classes": "govuk-!-margin-bottom-7"
      }) }}
      <a class="govuk-link" href="{{ url('eu_import_batch-ui-list') }}">
            Back to EU TARIC import list
          </a>
    </div>
  </div>
{% endblock %}
